# EconLens Backend Environment Configuration
# Copy this file to .env and update with your actual values

# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# CORS Configuration
# Comma-separated list of additional frontend URLs (optional)
# FRONTEND_URLS=https://dquykpffn9qx.cloudfront.net,https://your-alb-domain.com
# ALB_URL=https://your-alb-domain.com

# Database Configuration
DATABASE_URL=postgresql://econlens_admin:your_password@your_rds_endpoint:5432/econlens
DB_HOST=your_rds_endpoint
DB_PORT=5432
DB_NAME=econlens
DB_USER=econlens_admin
DB_PASSWORD=your_password

# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET_NAME=your_s3_bucket_name

# Cognito Configuration
COGNITO_USER_POOL_ID=your_user_pool_id
COGNITO_USER_POOL_CLIENT_ID=your_user_pool_client_id

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# Logging
LOG_LEVEL=info
