{
  "name": "@econlens/infrastructure",
  "version": "0.1.0",
  "description": "EconLens Simple AWS CDK Infrastructure for Learning",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf cdk.out dist node_modules",
    "cdk": "cdk",
    "deploy": "cdk deploy --profile econlens-admin",
    "destroy": "cdk destroy --profile econlens-admin",
    "diff": "cdk diff --profile econlens-admin",
    "synth": "cdk synth --profile econlens-admin",
    "bootstrap": "cdk bootstrap --profile econlens-admin"
  },
  "dependencies": {
    "@econlens/shared": "file:../shared",
    "aws-cdk-lib": "^2.114.1",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "aws-cdk": "^2.114.1"
  }
}
